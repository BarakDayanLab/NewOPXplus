    "keyboard": {
        "stop_experiment": {
            "description": "Stops experiment",
            "callback": "stop_experiment",
            "keys": ["escape", "Q", "q", "ctrl+q"]
        },
        "pause_unpause": {
            "description": "Toggles Pauses/Unpause, for taking of measurements",
            "callback": "pause_unpause",
            "keys": [" "]
        },
        "atoms_on_off": {
            "description": "Toggles atoms cloud on/off",
            "callback": "turn_on_off_atoms",
            "keys": ["A", "a"]
        },
        "zoom_plot": {
            "description": "Zoom in/out plots",
            "callback": "zoom_in_out_plots",
            "keys": ["0", "1", "2"]
        }
    },
